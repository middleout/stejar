{
  "name": "@stejar/version-card",
  "version": "1.0.8",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/version-card --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src",
    "deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/version-card --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src && npm version patch && npm publish --access=public"
  },
  "dependencies": {
    "@types/node": "6.0.49",
    "@types/react": "0.14.52",
    "react": "15.4.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.14",
    "webpack": "2.1.0-beta.19",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "1.0.2",
    "webpack-node-externals": "1.5.4",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "typescript": "rc",
    "ts-loader": "1.2.2"
  }
}
