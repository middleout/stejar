{
  "name": "@stejar/authentication",
  "version": "1.0.16",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/authentication --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src",
    "deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/authentication --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src && npm version patch && npm publish --access=public",
    "test": "karma start --single-run --no-auto-watch ./scripts/karma.config.js"
  },
  "dependencies": {
    "@stejar/redux": "^1.0.13",
    "@types/reselect": "2.0.27",
    "reselect": "2.5.4"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.14",
    "webpack": "2.1.0-beta.19",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "0.18.0",
    "webpack-node-externals": "1.5.4",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "ts-loader": "1.2.2",
    "typescript": "rc"
  }
}
