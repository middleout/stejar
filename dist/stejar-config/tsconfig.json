{
  "compileOnSave": false,
  "compilerOptions": {
	"target": "es6",
	"moduleResolution": "node",
	"declaration": true,
	"sourceMap": true,
	"charset": "utf8",
	"noEmitOnError": false,
	"noImplicitAny": true,
	"suppressImplicitAnyIndexErrors": true,
	"emitDecoratorMetadata": true,
	"experimentalDecorators": true,
	"forceConsistentCasingInFileNames": false,
	"allowSyntheticDefaultImports": true,
	"noUnusedParameters": false,
	"noUnusedLocals": false,
	"jsx": "preserve",
	"pretty": true,
	"listFiles": false,
	"allowJs": false,
	"baseUrl": ".",
	"lib": ["es2015", "dom", "es2016"]
  },
  "exclude": [
	"node_modules",
	"coverage",
	"webpack.config.js",
	"karma.config.js",
	"public",
	"tests"
  ]
}
