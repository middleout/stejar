{
  "name": "@stejar/di",
  "version": "1.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A complete dependency injection library for any framework",
  "files": [
    "*.md",
    "docs",
    "es",
    "lib",
    "types"
  ],
  "main": "lib/index",
  "module": "es/index",
  "jsnext:main": "es/index",
  "types": "types/index.d.ts",
  "repository": "middleout/stejar",
  "homepage": "https://github.com/middleout/stejar#readme",
  "bugs": "https://github.com/middleout/stejar/issues",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es && npm run build-types",
    "build-cjs": "rimraf es && tsc --outDir ./lib --module commonjs --target es5",
    "build-es": "rimraf es && tsc --outDir ./es --target es5",
    "build-types": "rimraf types && tsc --declaration --outDir ./types && rimraf types/*.js",
    "deploy": "karma start ./karma.conf.js && npm run build && bump --prompt --tag --push --all",
    "test": "karma start",
    "publish-please": "publish-please",
    "prepublish": "publish-please guard"
  },
  "authors": [
    "Andrei Gabreanu"
  ],
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.2",
    "reflect-metadata": "^0.1.9"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.41",
    "@types/node": "7.0.4",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "circular-dependency-plugin": "2.0.0",
    "clean-webpack-plugin": "0.1.15",
    "expect": "1.20.2",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine": "2.5.3",
    "jasmine-core": "2.5.2",
    "karma": "1.4.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "npm-check-updates": "2.8.9",
    "phantomjs-prebuilt": "2.1.14",
    "postcss-loader": "1.2.2",
    "publish-please": "2.2.0",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.6",
    "ts-loader": "2.0.0",
    "tslib": "1.5.0",
    "typescript": "^2.1.5",
    "version-bump-prompt": "^3.0.1",
    "webpack": "beta",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "2.6.0"
  },
  "tags": [
    "di",
    "dependency injection"
  ],
  "keywords": [
    "di",
    "dependencies",
    "dependency",
    "dependency injection"
  ]
}