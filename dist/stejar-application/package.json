{
  "name": "@stejar/application",
  "version": "1.0.0",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
	"build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/application --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src  && npm pack",
	"deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/application --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src  && npm pack",
	"test": "karma start --single-run --no-auto-watch ./scripts/karma.config.js"
  },
  "dependencies": {
	"@types/node": "6.0.48",
	"@types/react": "0.14.49",
	"@types/react-dom": "0.14.18",
	"babel-polyfill": "6.16.0",
	"es6-promise": "4.0.5",
	"react": "15.4.0",
	"react-dom": "15.4.0",
	"react-tap-event-plugin": "2.0.0"
  },
  "devDependencies": {
	"clean-webpack-plugin": "0.1.14",
	"webpack": "2.1.0-beta.19",
	"webpack-build-notifier": "0.1.13",
	"webpack-merge": "0.17.0",
	"webpack-node-externals": "1.5.4",
	"babel-core": "6.18.2",
	"babel-loader": "6.2.7",
	"babel-preset-es2015": "6.18.0",
	"babel-preset-es2015-loose": "8.0.0",
	"babel-plugin-transform-react-jsx": "6.8.0"
  }
}
