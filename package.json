{
  "name": "stejar",
  "version": "1.0.0",
  "description": "Web development framework for React",
  "private": true,
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "postinstall": "lerna bootstrap --stream --concurrency 1",
    "prepublishOnly": "yarn build",
    "bootstrap": "yarn install && lerna bootstrap --stream --concurrency 1",
    "link-bins": "link-parent-bin",
    "build": "lerna run build --stream --concurrency 1",
    "push": "lerna run build --stream && node ./scripts/yalc-push.js",
    "clean": "lerna run clean --stream && rimraf ./node_modules",
    "test": "lerna run test:run --stream --concurrency 1",
    "outdated": "lerna run outdated --stream --concurrency 1 && yarn outdated",
    "update": "lerna run update --stream --concurrency 1 && yarn upgrade && git add . && git commit -m \"Dependencies update\"",
    "docs": "gitbook build ./docs && gitbook serve ./docs"
  },
  "peerDependencies": {
    "yalc": "^1.0.0-pre.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^2.1.0",
    "colors": "^1.1.2",
    "depcheck": "^0.6.7",
    "diff": "^3.4.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "gitbook-cli": "^2.3.2",
    "github": "^11.0.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lerna": "^2.4.0",
    "link-parent-bin": "^0.1.3",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "prompt": "^1.0.0",
    "react": "^16.0.0",
    "rimraf": "^2.6.2",
    "semver": "^5.4.1",
    "terminal-kit": "^1.14.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
