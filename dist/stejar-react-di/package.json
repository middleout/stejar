{
  "name": "@stejar/react-di",
  "version": "1.0.5",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/react-di --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src",
    "deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/react-di --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src && npm version patch && npm publish --access=public"
  },
  "dependencies": {
    "@stejar/di": "^1.0.7",
    "@types/invariant": "2.2.28",
    "@types/lodash": "4.14.43",
    "@types/node": "6.0.49",
    "@types/react": "0.14.54",
    "hoist-non-react-statics": "1.2.0",
    "invariant": "2.2.2",
    "lodash": "4.17.2",
    "react": "15.4.1"
  },
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-loader": "6.2.9",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "clean-webpack-plugin": "0.1.14",
    "ts-loader": "1.3.1",
    "typescript": "2.1.4",
    "webpack": "2.1.0-beta.19",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "1.0.2",
    "webpack-node-externals": "1.5.4",
    "babel-plugin-transform-react-jsx": "6.8.0"
  }
}
