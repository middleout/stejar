{
  "name": "stejar",
  "version": "1.0.0",
  "description": "Web development framework for React",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "lerna bootstrap --stream",
    "prepublishOnly": "npm run build",
    "build": "lerna run build --stream --concurrency 1",
    "clean": "lerna run clean --stream",
    "test": "lerna run test:run --stream --concurrency 1",
    "upgrade": "lerna run upgrade --stream --concurrency 1",
    "update": "lerna run update --stream --concurrency 1",
    "docs": "gitbook build ./docs && gitbook serve ./docs",
    "publish": "npm run test && npm run build && lerna publish"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^2.1.0",
    "colors": "^1.1.2",
    "depcheck": "^0.6.7",
    "diff": "^3.3.1",
    "eslint-plugin-react": "^7.4.0",
    "gitbook-cli": "^2.3.2",
    "lerna": "^2.2.0",
    "npm-check-updates": "^2.12.1",
    "prompt": "^1.0.0",
    "react": "^16.0.0"
  }
}
