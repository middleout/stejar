{
  "name": "@stejar/authorization",
  "version": "1.0.0",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/authorization --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src && npm pack",
    "deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/authorization --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src  && npm pack",
    "test": "karma start --single-run --no-auto-watch ./scripts/karma.config.js"
  },
  "dependencies": {
    "@types/react": "^0.14.49",
    "react": "^15.4.0",
    "virgen-acl": "0.0.20"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "clean-webpack-plugin": "0.1.14",
    "typings": "2.0.0",
    "webpack": "2.1.0-beta.19",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "0.17.0",
    "webpack-node-externals": "1.5.4",
    "ts-loader": "^1.2.1",
    "typescript": "rc",
    "babel-plugin-transform-react-jsx": "6.8.0"
  }
}
