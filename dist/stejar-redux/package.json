{
  "name": "@stejar/redux",
  "version": "1.0.11",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/redux --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src",
    "deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/redux --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src && npm version patch && npm publish --access=public",
    "test": "karma start --single-run --no-auto-watch ./scripts/karma.config.js"
  },
  "dependencies": {
    "@stejar/di": "^1.0.1",
    "@types/node": "6.0.49",
    "babel-polyfill": "6.16.0",
    "invariant": "2.2.2",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-preset-es2015": "6.18.0",
    "clean-webpack-plugin": "0.1.14",
    "ts-loader": "1.2.2",
    "typescript": "rc",
    "webpack": "2.1.0-beta.19",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "0.17.0",
    "webpack-node-externals": "1.5.4"
  }
}
