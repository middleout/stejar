{
  "name": "@stejar/translate",
  "version": "1.0.10",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "extract-locale": "./bin/extract-locale.js"
  },
  "scripts": {
    "build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/translate --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src",
    "deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/translate --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src && npm version patch && npm publish --access=public"
  },
  "dependencies": {
    "@stejar/di": "^1.0.4",
    "@stejar/react": "^1.0.5",
    "@stejar/react-di": "^1.0.4",
    "@stejar/redux": "^1.0.16",
    "@stejar/utils": "^1.0.14",
    "@types/lodash": "4.14.41",
    "@types/moment": "2.13.0",
    "@types/numbro": "0.0.28",
    "@types/react": "0.14.52",
    "@types/react-helmet": "0.0.25",
    "@types/react-redux": "^4.4.35",
    "@types/reselect": "^2.0.27",
    "lodash": "4.17.2",
    "moment": "2.17.0",
    "numbro": "1.9.3",
    "react": "15.4.1",
    "react-helmet": "3.2.2",
    "react-redux": "^4.4.6",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "clean-webpack-plugin": "0.1.14",
    "ts-loader": "1.2.2",
    "typings": "2.0.0",
    "webpack": "2.1.0-beta.19",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "1.0.2",
    "webpack-node-externals": "1.5.4",
    "typescript": "rc"
  }
}
