{
  "name": "stejar",
  "version": "1.0.0",
  "description": "Web development framework for React",
  "private": true,
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "postinstall": "lerna bootstrap --stream",
    "prepublishOnly": "npm run build",
    "bootstrap": "lerna bootstrap --stream --concurrency 1",
    "build": "lerna run build --stream --concurrency 1",
    "clean": "lerna run clean --stream && rimraf ./node_modules",
    "test": "lerna run test:run --stream --concurrency 1",
    "upgrade": "lerna run upgrade --stream --concurrency 1",
    "update": "lerna run update --stream --concurrency 1 && git add . && git commit -m \"Dependencies update\"",
    "docs": "gitbook build ./docs && gitbook serve ./docs",
    "publish": "npm run test && npm run build && lerna publish"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^2.1.0",
    "colors": "^1.1.2",
    "depcheck": "^0.6.7",
    "diff": "^3.3.1",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "gitbook-cli": "^2.3.2",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lerna": "^2.2.0",
    "lint-staged": "^4.2.3",
    "npm-check-updates": "^2.13.0",
    "prettier": "^1.7.4",
    "prompt": "^1.0.0",
    "react": "^16.0.0",
    "rimraf": "^2.6.2"
  }
}
