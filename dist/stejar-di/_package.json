{
  "name": "@stejar/di",
  "version": "1.0.10",
  "main": "dist/es5/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "npm run build:local",
    "build:local": "rm -rf ./dist && tsc --outDir ./dist/es5 && tsc --outDir ./dist/es6 --target es6 --declaration false",
