{
    "main": "./lib/index.js",
    "module": "./es/index.js",
    "jsnext:main": "./es/index.js",
    "typings": "./typings/index.d.ts",
    "directories": {
        "test": "tests"
    },
    "files": [
        "extra-typings",
        "es",
        "lib",
        "typings"
    ],
    "scripts": {
        "compile": "rimraf ./compiled && tsc --outDir ./compiled && babel ./compiled --source-maps inline --out-dir ./es",
        "typings": "rimraf ./typings && tsc --declaration --declarationDir ./typings --outDir ./compiled && rimraf ./compiled",
        "build": "rimraf ./lib && webpack && npm run typings",
        "integrate": "npm run build && cd ./../../__integration && npm install ./../packages/http",
        "test": "jest",
        "test-start": "jest --watch",
        "start": "webpack --watch"
    },
    "author": "Middleout",
    "license": "MIT",
    "dependencies": {
        "@types/whatwg-fetch": "0.0.33",
        "isomorphic-fetch": "^2.2.1"
    },
    "devDependencies": {
        "@types/enzyme": "^2.8.4",
        "@types/jest": "^20.0.4",
        "@types/react-test-renderer": "^15.5.2",
        "babel-core": "^6.25.0",
        "babel-jest": "^20.0.3",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "case-sensitive-paths-webpack-plugin": "^2.1.1",
        "circular-dependency-plugin": "^3.0.0",
        "enzyme": "^2.9.1",
        "jest": "^20.0.4",
        "prettier": "^1.5.3",
        "react": "^15.6.1",
        "react-addons-test-utils": "^15.6.0",
        "react-dom": "^15.6.1",
        "react-test-renderer": "^15.6.1",
        "rimraf": "^2.6.1",
        "ts-jest": "^20.0.7",
        "ts-loader": "^2.3.1",
        "tslib": "^1.7.1",
        "tslint": "^5.5.0",
        "tslint-config-prettier": "^1.1.0",
        "tslint-loader": "^3.5.3",
        "typescript": "^2.4.2",
        "webpack": "^3.3.0",
        "webpack-node-externals": "^1.6.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testMatch": [
            "**/__tests__/*.(ts|tsx|js)"
        ],
        "collectCoverageFrom": [
            "**/src/**"
        ],
        "collectCoverage": true,
        "coverageDirectory": "__tests__/__coverage__",
        "coverageReporters": [
            "lcov"
        ],
        "mapCoverage": true
    },
    "version": "2.0.1",
    "description": "",
    "name": "@stejar/http"
}