    "test": "karma start --single-run --no-auto-watch karma.js"
  },
  "devDependencies": {
	"@types/invariant": "2.2.28",
	"invariant": "2.2.2",
	"reflect-metadata": "0.1.8",
    "@types/expect": "1.13.31",
    "@types/jasmine": "2.5.38",
    "babel-polyfill": "6.20.0",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "npm-check-updates": "2.8.8",
    "phantomjs-polyfill-find": "0.0.1",
    "phantomjs-prebuilt": "2.1.14",
    "typescript": "2.1.4"
