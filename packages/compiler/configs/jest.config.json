{
    "moduleNameMapper": {
        "\\.(css|scss|less)$": "identity-obj-proxy",
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/jest/fileMock.js"
    },
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx"
    ],
    "setupFiles": [
        "<rootDir>/scripts/jest/beforeTest.js"
    ],
    "transform": {
        ".(js|jsx)": "<rootDir>/scripts/jest/babelTransform.js"
    },
    "testMatch": [
        "<rootDir>/../../../src/**/*.test.(js|jsx)"
    ],
    "collectCoverageFrom": [
        "**/src/**/*.{js,jsx}",
        "!**/src/**/*.story.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/../../../__coverage__",
    "coverageReporters": [
        "text",
        "lcov"
    ],
    "mapCoverage": true,
    "rootDir": "./../",
    "roots": [
        "<rootDir>/../../../"
    ]
}
