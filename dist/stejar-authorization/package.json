{
  "name": "@stejar/authorization",
  "version": "1.0.10",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/authorization --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src",
    "deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/authorization --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src && npm version patch && npm publish --access=public",
    "test": "karma start --single-run --no-auto-watch ./scripts/karma.config.js"
  },
  "dependencies": {
    "@stejar/di": "^1.0.9",
    "@stejar/typings": "^1.0.8",
    "virgen-acl": "0.0.20"
  },
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-loader": "6.2.9",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "clean-webpack-plugin": "0.1.14",
    "typings": "2.0.0",
    "webpack": "2.1.0-beta.19",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "1.1.1",
    "webpack-node-externals": "1.5.4",
    "ts-loader": "1.3.2",
    "typescript": "rc"
  }
}
