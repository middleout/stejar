{
  "name": "stejar",
  "version": "1.0.0",
  "description": "Web development framework for React",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "lerna bootstrap --stream --concurrency 1",
    "prepublishOnly": "yarn build",
    "bootstrap": "yarn install && lerna bootstrap --stream --concurrency 1",
    "link-bins": "link-parent-bin",
    "build": "lerna run build --stream --concurrency 1",
    "push": "lerna run build --stream && node ./scripts/yalc-push.js",
    "clean": "lerna run clean --stream && rimraf ./node_modules",
    "test": "lerna run test:run --stream --concurrency 1",
    "outdated": "lerna run outdated --stream --concurrency 1 && yarn outdated",
    "update": "lerna run update --stream --concurrency 1 && yarn upgrade && git add . && git commit -m \"Dependencies update\"",
    "docs": "gitbook build ./docs && gitbook serve ./docs"
  },
  "peerDependencies": {
    "yalc": "^1.0.0-pre.11"
  },
  "devDependencies": {
    "gitbook-cli": "^2.3.2",
    "lerna": "^2.4.0"
  },
  "dependencies": {}
}
