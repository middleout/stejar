{
  "name": "@stejar/http",
  "version": "1.0.3",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/http --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src",
    "deploy": "webpack --config=./webpack.js --progress && dts-bundle --name @stejar/http --main ./dist/index.d.ts --out ./index.d.ts --outputAsModuleFolder && rm -rf ./dist/src && npm version patch && npm publish --access=public"
  },
  "dependencies": {
    "@types/lodash": "4.14.43",
    "@types/node": "6.0.48",
    "@types/whatwg-fetch": "0.0.32",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.14",
    "webpack": "2.1.0-beta.19",
    "webpack-build-notifier": "0.1.13",
    "webpack-merge": "1.1.1",
    "webpack-node-externals": "1.5.4",
    "babel-core": "6.20.0",
    "babel-loader": "6.2.9",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "8.0.0",
    "ts-loader": "1.3.2",
    "typescript": "rc"
  }
}
